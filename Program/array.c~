#include<stdio.h>
#include<string.h>
int main()
{
 char options[2][10];
 char name[2][32]={"Raj uprade","sheetal singh"}; 
 options[0][0]='t';
options[0][1]='e';
options[0][2]='s';
options[0][3]='t';
options[0][4]='1';
options[0][5]='\0';  /* NUL termination of C string */

/* A standard C library function which copies strings. */
strcpy(options[1], "test2");

printf("%s %s\n",options[0],options[1]);
printf("%s %s\n",name[0],name[1]);

return 0;
}
